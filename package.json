{
  "name": "microservice-auth-service",
  "version": "1.0.0",
  "description": "Microservice auth service",
  "main": "server.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "db-migrate": "prisma migrate dev --skip-seed --name migration",
    "db-migrate-reset": "prisma migrate reset --skip-seed",
    "db-seed": "prisma db seed",
    "db-generate": "prisma generate",
    "prisma": "prisma",
    "test": "vitest run",
    "test:e2e": "vitest run src/tests/e2e",
    "test:integration": "vitest run src/tests/integration",
    "test:unit": "vitest run src/tests/unit",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christianmurillo10/microservice-auth-service.git"
  },
  "author": "Christian Murillo",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seeders"
  },
  "bugs": {
    "url": "https://github.com/christianmurillo10/microservice-auth-service/issues"
  },
  "homepage": "https://github.com/christianmurillo10/microservice-auth-service#readme",
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ioredis": "^5.7.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "prisma": "^6.5.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  }
}
